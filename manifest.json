{
    "manifest_version": 2,

    "name": "Bowdoin Dining",
    "description": "This extension for Bowdoin Students lets you access the Dining Hall menus from an extension.",
    "version": "1.1.3",

    "permissions": [
        "http://diningwithstrangers.co/"
    ],
    "browser_action": {
        "default_icon": "128.png",
        "default_popup": "menus.html"
    },
    "icons": {
        "48": "48.png",
        "128": "128.png"
    }

}